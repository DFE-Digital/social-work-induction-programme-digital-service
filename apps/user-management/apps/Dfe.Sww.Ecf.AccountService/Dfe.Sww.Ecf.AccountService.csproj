        <ImplicitUsings>enable</ImplicitUsings>
        <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
        <UserSecretsId>e812e454-6fd2-4bba-8173-9ea4d87f32b1</UserSecretsId>
